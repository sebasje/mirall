<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="org.kde.owncloudsync">
    <method name="refresh">
    </method>
    <method name="display">
      <arg type="s" direction="out" />
    </method>
    <signal name="displayChanged">
      <arg type="s" direction="out" />
    </signal>
    <signal name="statusChanged">
      <arg type="i" direction="out" />
    </signal>
    <signal name="errorChanged">
      <arg type="i" direction="out" />
    </signal>

    <signal name="owncloudChanged">
      <annotation name="org.qtproject.QtDBus.QtTypeName.In0" value="QVariantMap"/>
      <arg type="a{sv}" direction="out" />
    </signal>

    <method name="setupOwncloud">
      <arg name="server" type="s" direction="in" />
      <arg name="user" type="s" direction="in" />
      <arg name="password" type="s" direction="in" />
    </method>

    <method name="folder">
      <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="QVariantMap"/>
      <arg name="name" type="s" direction="in" />
      <arg type="a{sv}" direction="out" />
    </method>

    <method name="enableFolder">
      <arg name="name" type="s" direction="in" />
      <arg name="enable" type="b" direction="in" />
    </method>

    <method name="syncFolder">
      <arg name="name" type="s" direction="in" />
    </method>

    <method name="cancelSync">
      <arg name="name" type="s" direction="in" />
    </method>

    <method name="addSyncFolder">
      <arg name="localFolder" type="s" direction="in" />
      <arg name="remoteFolder" type="s" direction="in" />
      <arg name="alias" type="s" direction="in" />
    </method>

    <method name="removeSyncFolder">
      <arg name="alias" type="s" direction="in" />
    </method>

    <method name="createRemoteFolder">
      <arg name="name" type="s" direction="in" />
    </method>

    <method name="checkRemoteFolder">
      <arg name="name" type="s" direction="in" />
    </method>

    <signal name="remoteFolderExists">
      <arg type="s" direction="out" />
      <arg type="b" direction="out" />
    </signal>

    <method name="folderList">
      <annotation name="org.qtproject.QtDBus.QtTypeName.Out0" value="QVariantMap"/>
      <arg type="a{sv}" direction="out" />
    </method>
    <signal name="folderListChanged">
      <annotation name="org.qtproject.QtDBus.QtTypeName.In0" value="QVariantMap"/>
      <arg type="a{sv}" direction="out" />
    </signal>

    <signal name="folderChanged">
      <annotation name="org.qtproject.QtDBus.QtTypeName.In0" value="QVariantMap"/>
      <arg type="a{sv}" direction="out" />
    </signal>

    <signal name="progressChanged">
      <annotation name="org.qtproject.QtDBus.QtTypeName.In0" value="QVariantMap"/>
      <arg type="a{sv}" direction="out" />
    </signal>


  </interface>
</node>
